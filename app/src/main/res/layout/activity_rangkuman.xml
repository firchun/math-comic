<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".rangkuman">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="0dp"
        android:padding="0dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >
            <ImageView
                android:id="@+id/rangkuman_1"
                android:layout_width="match_parent"
                android:layout_height="1050dp"
                app:srcCompat="@drawable/rangkuman_1" />
             <ImageView
                android:id="@+id/rangkuman_2"
                android:layout_width="match_parent"
                android:layout_height="1050dp"
                app:srcCompat="@drawable/rangkuman_2" />
<ImageView
                android:id="@+id/rangkuman_3"
                android:layout_width="match_parent"
                android:layout_height="1050dp"
                app:srcCompat="@drawable/rangkuman_3" />
<ImageView
                android:id="@+id/rangkuman_4"
                android:layout_width="match_parent"
                android:layout_height="1050dp"
                app:srcCompat="@drawable/rangkuman_4" />
<ImageView
                android:id="@+id/rangkuman_5"
                android:layout_width="match_parent"
                android:layout_height="1050dp"
                app:srcCompat="@drawable/rangkuman_5" />
<ImageView
                android:id="@+id/rangkuman_6"
                android:layout_width="match_parent"
                android:layout_height="1050dp"
                app:srcCompat="@drawable/rangkuman_6" />
<ImageView
                android:id="@+id/rangkuman_7"
                android:layout_width="match_parent"
                android:layout_height="1050dp"
                app:srcCompat="@drawable/rangkuman_7" />
<ImageView
                android:id="@+id/rangkuman_8"
                android:layout_width="match_parent"
                android:layout_height="1050dp"
                app:srcCompat="@drawable/rangkuman_8" />

        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>